{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch via NPM",
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "--inspect",
                "./dist"
            ],
            "protocol": "inspector",
            "port": 9229,
            "windows": {
                "runtimeExecutable": "npm.cmd"
            },
            "linux": {
                "runtimeExecutable": "npm"
            }
        }
    ]
}